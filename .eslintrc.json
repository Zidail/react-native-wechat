{
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "no-proto": 0,
    "class-methods-use-this": 0,
    "max-len": ["error", 180],
    "import/no-unresolved": [
      2,
      {
        "ignore": ["^react$", "^react-native$", "^react-native/"]
      }
    ],
    "import/no-named-as-default-member": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
